{
  "crons": [
    {
      "path": "/api/cron/cleanup-rate-limits",
      "schedule": "0 */6 * * *"
    },
    {                                                                                                    
      "path": "/api/cron/cleanup-shares",                                                                          
      "schedule": "0 3 * * *"                                                                                      
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "bentoanime.com" }],
      "destination": "https://www.bentoanime.com/:path*",
      "permanent": true
    }
  ]
}
